<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Tab text="CUSTOMER" xmlns="http://javafx.com/javafx"
     xmlns:fx="http://javafx.com/fxml"
     fx:controller="com.hoster.app.controllers.CustomerController">
    <content>
        <AnchorPane id="Customer" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
                <TableView fx:id="dataTableCustomer" layoutX="0.0" layoutY="42.0" prefHeight="360.0" prefWidth="861.0">
                    <columns>
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="20.0" text="#" fx:id="id" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="105.0" text="Nama Depan" fx:id="namaDepan" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="117.0" text="Nama Belakang" fx:id="namaBelakang" />
                        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="76.0" text="Jenis Kelamin" fx:id="jk" />
                        <TableColumn prefWidth="75.0" text="No Telp" fx:id="noTelp" />
                        <TableColumn prefWidth="75.0" text="No KTP" fx:id="noKTP" />
                        <TableColumn prefWidth="75.0" text="Email" fx:id="email" />
                        <TableColumn prefWidth="75.0" text="Alamat" fx:id="alamat" />
                        <TableColumn prefWidth="75.0" text="Kota" fx:id="kota" />
                        <TableColumn maxWidth="100.0" prefWidth="97.0" text="Negara" fx:id="negara" />
                    </columns>
                </TableView>
                <TextField fx:id="queryCustomer" layoutX="105.0" onAction="#cariCustomer" layoutY="14.0" prefWidth="200.0" />
                <Label layoutX="14.0" layoutY="17.0" text="Kata Kunci" />
                <Button fx:id="btnCariCustomer" layoutX="314.0" layoutY="15.0" onAction="#cariCustomer" mnemonicParsing="false" text="Cari" />
            </children>
        </AnchorPane>
    </content>
</Tab>
